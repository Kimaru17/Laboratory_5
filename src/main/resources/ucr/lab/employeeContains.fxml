<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<BorderPane fx:id="bp"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="controller.EmployeeContains"
            prefHeight="400.0" prefWidth="600.0">
    <center>
        <Pane prefHeight="463.0" prefWidth="749.0" style="-fx-background-color: #B2DFDB;" BorderPane.alignment="CENTER">
            <children>
                <Label layoutX="178.0" layoutY="272.0" text="Employee ID:">
                    <font>
                        <Font name="Arial" size="20.0" />
                    </font>
                </Label>
                <TextField fx:id="tf_employeeId" layoutX="354.0" layoutY="271.0" />
                <Pane layoutY="489.0" prefHeight="102.0" prefWidth="747.0" style="-fx-background-color: #4DB6AC;">
                    <children>
                        <Button layoutX="139.0" layoutY="14.0" mnemonicParsing="false" onAction="#containsOnAction" prefHeight="57.0" prefWidth="114.0" text="Verify">
                            <font>
                                <Font name="Arial" size="18.0" />
                            </font>
                        </Button>
                        <Button layoutX="318.0" layoutY="15.0" mnemonicParsing="false" onAction="#cleanOnAction" prefHeight="57.0" prefWidth="114.0" text="Clean">
                            <font>
                                <Font name="Arial" size="18.0" />
                            </font>
                        </Button>
                        <Button layoutX="495.0" layoutY="14.0" mnemonicParsing="false" onAction="#closeOnAction" prefHeight="57.0" prefWidth="114.0" text="Close">
                            <font>
                                <Font name="Arial" size="18.0" />
                            </font>
                        </Button>
                    </children>
                </Pane>
                <Pane prefHeight="121.0" prefWidth="747.0" style="-fx-background-color: #00796B;">
                    <children>
                        <Label layoutX="255.0" layoutY="42.0" text="Contains Employee">
                            <font>
                                <Font name="Arial" size="40.0" />
                            </font>
                        </Label>
                    </children>
                </Pane>
                <Label layoutX="134.0" layoutY="203.0" text="Enter the id of the employee you want to know if exists:">
                    <font>
                        <Font name="Arial" size="20.0" />
                    </font>
                </Label>
            </children>
        </Pane>
    </center>
</BorderPane>